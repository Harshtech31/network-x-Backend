{
  "name": "network-x-backend",
  "version": "1.0.0",
  "description": "Backend API for Network-x student networking platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "reset-db": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "keywords": [
    "nodejs",
    "express",
    "api",
    "student-networking",
    "collaboration"
  ],
  "author": "Network-x Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "sequelize": "^6.35.2",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.6",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.1.5",
    "multer": "^1.4.5-lts.1",
    "cloudinary": "^1.41.0",
    "socket.io": "^4.7.4",
    "redis": "^4.6.10",
    "nodemailer": "^6.9.7",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "sequelize-cli": "^6.6.2",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
