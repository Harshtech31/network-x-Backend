{
  "name": "network-x-backend",
  "version": "1.0.0",
  "description": "Backend API for Network-x student networking platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "reset-db": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "keywords": [
    "nodejs",
    "express",
    "api",
    "student-networking",
    "collaboration"
  ],
  "author": "Network-x Team",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1691.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.0.8",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^2.5.11",
    "express-rate-limit": "^5.5.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongodb": "^6.19.0",
    "mongoose": "^3.6.20",
    "morgan": "^1.3.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.7",
    "redis": "^4.6.10",
    "socket.io": "^0.9.19",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "jest": "^25.0.0",
    "nodemon": "^1.4.1",
    "sequelize-cli": "^6.2.0",
    "supertest": "^0.7.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
